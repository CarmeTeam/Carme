/*--------------------------------------------------*/
/*                     General                      */
/*--------------------------------------------------*/

body {
  background-color: var(--carme-body-bg)!important;
}
.text-header {
  color: var(--carme-card-head-text)!important;
}
.text-body {
  color: var(--carme-card-body-text)!important;
}
.text-watermark {
  color: var(--carme-watermark-text)!important;
}
.text-title {
  color: var(--carme-title-text)!important;
}
.text-brand {
  color: var(--carme-brand-text)!important;
}
a{
  color: var(--carme-primary);
}

/*--------------------- Borders --------------------*/
.border-bottom-double{
  border-bottom: 2px solid var(--carme-border-head);
}
.border-bottom-solid{
  border-bottom: 1px solid var(--carme-border-body);
}
.border-bottom-dashed{
  border-bottom: 1px dashed var(--carme-border-body);
}
.border-top-double{
  border-top: 2px solid var(--carme-border-head);
}
.border-top-solid{
  border-top: 1px solid var(--carme-border-body);
}
.border-top-dashed{
  border-top: 1px dashed var(--carme-border-body);
}


/*-----------------  Card & Modal  -----------------*/

/* card & modal */
.card, .modal-content {
  background-color: var(--carme-card-bg);
  border: 1px solid var(--carme-card-border);
  box-shadow: var(--carme-card-shadow)!important; 
  -webkit-box-shadow: var(--carme-card-shadow)!important;
  border-radius: 10px;
  overflow:hidden;  
  text-align: Left;
}
.card-header, .modal-header {
  background-color: var(--carme-card-header-bg);
  border-bottom: 1px solid var(--carme-card-header-border);
  box-shadow: var(--carme-card-header-shadow)!important; 
  -webkit-box-shadow: var(--carme-card-header-shadow)!important;
}
.card-footer, .modal-footer {
  background-color: var(--carme-card-footer-bg);
  border-top: 1px solid var(--carme-card-footer-border);
  box-shadow: var(--carme-card-footer-shadow)!important; 
  -webkit-box-shadow: var(--carme-card-footer-shadow)!important;
}


/* body-scrollbar not hidden when modal is active*/
.modal-open {
  overflow:hidden!important; 
  overflow-y:auto!important;
  /* padding-right:0 !important; */
}

/*-------------------  Form-Select  ------------------*/
.form-select, 
.form-select:focus  {
  padding: .3125rem 2rem .3125rem 0.6rem;
  background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 16 16'%3e%3cpath fill='none' stroke='%234d5969' stroke-linecap='round' stroke-linejoin='round' stroke-width='2' d='M2 5l6 6 6-6'/%3e%3c/svg%3e");
  background-repeat: no-repeat, repeat;
  background-position: right 0.6rem center;
  background-size: 16px 12px;
  border-radius: .25rem;
  -moz-appearance: none;
  -webkit-appearance: none;
  appearance: none;
  border: 1px solid var(--carme-form-border);
  background-color: var(--carme-form-bg)!important;
  box-shadow: var(--carme-form-shadow)!important;
  outline:none;
}
.form-select:hover, 
.form-select:active {
  border-color: var(--carme-form-border-hover);
  outline: 0;
} 
.form-select.bg-transparent {
  background-color: transparent!important;
}
.form-select option.bg-transparent {
  background-color: var(--carme-form-bg)!important;
}

/*--------------------------------------------------*/
/*                     Login                        */
/*--------------------------------------------------*/

/* input-text */
.form-group input,
.form-group input:valid,
.form-group input:focus  {
  color: var(--carme-input-text);
  height: 3.125rem; 
}

/* input-label */
.form-group label {
  color: var(--carme-input-label-lg-text);
  font-size: 16px;
  font-weight: 300;
  transform: translateY(8px);
  transition: all 0.2s ease-in-out;
  position: absolute;
  pointer-events: none;
  top: 0; 
  height: 3.125rem;
}
.form-group input:valid + label,
.form-group input:focus + label,
.form-group input:-webkit-autofill + label {
  color: var(--carme-input-label-sm-text);
  font-size: 12px;
  font-weight: 300;
  transform: translateY(0);
}

/* chrome-autofill */
input:-webkit-autofill,
input:-webkit-autofill:hover,
input:-webkit-autofill:focus,
input:-webkit-autofill:active {
  transition: background-color 5000s ease-in-out 0s;
  -webkit-text-fill-color: var(--carme-input-text);
  -webkit-box-shadow: inset 0 0 0 30px var(--carme-input-bg)!important;
}

/* firefox-autofill */
input:autofill {
  background: var(--carme-input-bg);
}

/* chrome-type=number */
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
  -webkit-appearance: none;
}

/* firefox-type=number */
input[type=number] {
  -moz-appearance: textfield;
}

/* input-border */
.form-control {
  -moz-appearance: none;
  -webkit-appearance: none;
  appearance: none;
  background-color: transparent!important;
  border: 1px solid var(--carme-input-border);
  border-radius: .25rem;
  -webkit-box-shadow: var(--carme-input-shadow);
  box-shadow: var(--carme-input-shadow);
  outline:none;
}

.form-control:focus {
  border-color: var(--carme-input-border-focus);
  -webkit-box-shadow: var(--carme-input-shadow);
  box-shadow: var(--carme-input-shadow);
  outline: none; 
  outline: 0; 
}

/* disable input shadow in login box */
.form-control-login {
  -webkit-box-shadow: none!important;
  box-shadow: none!important;
}

/* centered box */
.flex-center-carme {
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
}

.min-vh-85 {
  min-height: 85vh !important;
}

#login-box {
  border-radius: 12px; 
  padding: 15px;
  max-width: 400px;
  margin: auto;
  -webkit-box-shadow: var(--carme-login-box-shadow);
  box-shadow: var(--carme-login-box-shadow);
  border: 1px solid var(--carme-login-box-border);  
  background-color: var(--carme-login-box-bg)!important;
}

/* QR code */
.qrbox {
  border: 4px solid transparent; 
  --g: var(--carme-black) 90deg, var(--carme-qrcode-border) 0;
  background:
    conic-gradient(from 90deg  at top    4px left  4px,var(--g)) 0    0,
    conic-gradient(from 180deg at top    4px right 4px,var(--g)) 100% 0,
    conic-gradient(from 0deg   at bottom 4px left  4px,var(--g)) 0    100%,
    conic-gradient(from -90deg at bottom 4px right 4px,var(--g)) 100% 100%;
  background-size: 30% 30%;
  background-origin: border-box;
  background-repeat: no-repeat;
  -webkit-box-shadow: var(--carme-qrcode-shadow);
  box-shadow: var(--carme-qrcode-shadow);
  margin: 5px;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  
}
.qrcode{
  width:150px; 
  height:150px; 
  background-color:var(--carme-qrcode-bg);
}

/* input-group */
.input-group-text {
  background-repeat: no-repeat, repeat;
  background-position: right 0.6rem center;
  background-size: 16px 12px;
  border-radius: .25rem;
  -moz-appearance: none;
  -webkit-appearance: none;
  appearance: none;
  border: 1px solid var(--carme-input-group-border);
  color: var(--carme-input-group-text);
  background-color: var(--carme-input-group-bg)!important;
  box-shadow: var(--carme-input-group-shadow)!important;
  outline:none;
}

/*--------------------------------------------------*/
/*                     Footer                       */
/*--------------------------------------------------*/

/* top */
.footer-top{
  color: var(--carme-footer-top-text);
  border-top: var(--carme-footer-top-border);
  box-shadow: var(--carme-footer-top-shadow);
  background-color: var(--carme-footer-top-bg);
  -webkit-box-shadow: var(--carme-footer-top-shadow);
}
.footer-top a {
  color: var(--carme-footer-top-text);
}
.footer-top a:hover {
  color: var(--carme-footer-top-text-hover);
}

/* bottom */
.footer-bottom{
  color: var(--carme-footer-bottom-text);
  border-top: var(--carme-footer-bottom-border);
  box-shadow: var(--carme-footer-bottom-shadow);
  background-color: var(--carme-footer-bottom-bg);
  -webkit-box-shadow: var(--carme-footer-bottom-shadow);
}
.footer-bottom a {
  color: var(--carme-footer-bottom-text);
}
.footer-bottom a:hover {
  color: var(--carme-footer-bottom-text-hover);
} 

/*--------------------------------------------------*/
/*                     Navbar                       */
/*--------------------------------------------------*/

/* nav */
nav {
  background-color: var(--carme-navbar-bg)!important;
  border-bottom: none;
		padding-right:0 !important;
}

/* navbar-link */
nav a.nav-link,
nav a.nav-link:focus,
nav a.nav-link:active{
  color: var(--carme-navbar-text)!important;
}
nav a.nav-link:hover{
  color: var(--carme-navbar-text-hover)!important;
} 

/* navbar-toggle */
.navbar-toggler-icon {
  display: block;
  width: 20px;
  height: 40px;
  margin: -5px;
}
.navbar-toggler-frame {
  height: 2.5rem;
  width: 2.5rem;
  padding: 0.3125rem;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  border-radius: 50%;
}
.navbar-toggler-frame:hover {
  border: 1px solid var(--carme-toggler-border-hover)!important;
}
.navbar-toggler-frame:focus, 
.navbar-toggler-frame:active {
  outline: none;
  box-shadow: none;
}

/* navbar-right-icons */
.fa-border {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  border-radius: 50%;
  background-color: var(--carme-navbar-icons-bg);
  border: none;
  height: 30px;
  width: 30px;
}

/* dropdown-arrow-icon */
.dropdown-toggle.dropdown-right::after {
  content: none;
}
.dropdown-toggle.dropdown-left::after {
  border: none!important;
  font: normal normal normal 14px/1 FontAwesome;
  content: "\f107"!important; 
  vertical-align: 0; 
}

/* dropdown-menu */
.dropdown-menu{
  position: absolute;
  top: 112% !important;
  z-index: 1000;
  display: none;
  min-width: 10rem;
  padding: 1rem 0;
  margin: 0;
  font-size: 0.8rem;
  text-align: left;
  list-style: none;
  background-clip: padding-box;
  border-radius: .25rem;
  color: var(--carme-dropdown-menu-text)!important;
  background-color: var(--carme-dropdown-menu-bg)!important;
  border: 1px solid var(--carme-dropdown-menu-border)!important;
  -webkit-box-shadow: var(--carme-dropdown-menu-shadow)!important;
  box-shadow: var(--carme-dropdown-menu-shadow)!important;
}

/* dropdown-menu shape */
.dropdown-menu:after {
  content: "";
  position: absolute;
  z-index: -1;
  width: 1rem;
  height: 1rem;
  background: #fff;
  top: -0.53rem;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
  background: inherit;
  border-radius: 0.125rem;
  border-width: 1px 0 0 1px;
  border-style: solid;
  border-color: var(--carme-dropdown-menu-border);
}

/* dropdown-menu shape in small screen */
@media (max-width: 768px){
  .dropdown-menu:after{
    display: none!important;
		}
} 

/* dropdown-menu position */
.dropdown-menu.dropdown-menu-user:after {
  right: 1.05rem;
}
.dropdown-menu.dropdown-menu-settings:after {
  left: 1.60rem;
}
.dropdown-menu.dropdown-menu-notifications:after {
  right: 4.25rem;
}
.dropdown-menu.dropdown-menu-links:after {
  left: 1.2rem;
}
.dropdown-menu-notifications {
  left: 69% !important;
  right: auto !important;
  transform: translate(-69%, 0) !important;
}
.dropdown-menu-settings {
  left: 9% !important;
  right: auto !important;
  transform: translate(-9%, 0) !important;
}

.dropdown-menu-user {
  left: 98% !important;
  right: auto !important;
  transform: translate(-98%, 0) !important;
}

/*dropdown-item*/
.dropdown-item{
  color: var(--carme-dropdown-menu-text)!important;
}
.dropdown-item:hover, 
.dropdown-item:focus, 
.dropdown-item:active{
  background-color: var(--carme-dropdown-menu-bg-hover) !important;
  color: var(--carme-dropdown-menu-text-hover)!important;
}

/* dropdown-divider */
.dropdown-divider {
  border-top: 1px solid var(--carme-dropdown-divider-border)!important;
} 


/*--------------------------------------------------*/
/*               System Card - Dashboard            */
/*--------------------------------------------------*/

.option-acc {
  display: none;
}
.option-nod{
  display: none;
}
.dot {
  width: 0.625rem;
  height: 0.625rem;
  border-radius: 50%;
  display: inline-block;
  margin-right: 0.5rem;
}
.dot-others{
  background-color: var(--carme-bullet-bg)!important;
}
.border-top {
  border-top: 1px solid var(--carme-border)!important;
}


/*--------------------------------------------------*/
/*               Chart Card - Dashboard             */
/*--------------------------------------------------*/

/* tabs */
.nav-tabs {
  border-bottom: 1px solid var(--carme-tab-border);
}

.nav-tabs .nav-item .nav-link{
  background-color: var(--carme-tab-bg)!important;
}

.nav-tabs .nav-link.active,
.nav-tabs .nav-item.show .nav-link {
  background-color: var(--carme-tab-bg);;
  border-color: var(--carme-tab-border) var(--carme-tab-border) var(--carme-card-bg)!important;
}

.nav-tabs .nav-item .nav-link:hover {
  background-color: var(--carme-tab-bg);
  border-top-color: var(--carme-tab-border-hover);
  border-right-color: var(--carme-tab-border-hover);
  border-bottom-color: transparent;
  border-left-color: var(--carme-tab-border-hover);
  -webkit-box-shadow: var(--carme-tab-shadow);
  box-shadow: var(--carme-tab-shadow); 
}

/* highchart */
.option-chart {
  display: none;
}
.highcharts-legend-item path.highcharts-graph {
	display: none;
}


/*--------------------------------------------------*/
/*               Jobs Card - Dashboard              */
/*--------------------------------------------------*/
.option-proj{
  display: none;
} 

/*--- fraction ---*/
span.frac {
  display: inline-block;
  font-size: 100%;
  text-align: center;
}
span.frac > sup {
  display: block;
  border-bottom: 1px solid;
  margin-top: 0px;
  font: inherit;
  top: 0px;
}
span.frac > span {
  display: none;
}
span.frac > sub {
  display: block;
  font: inherit;
  bottom: 0px
}


/*----------------------*/
/*       Buttons        */
/*----------------------*/

/* custom-button */
.btn-custom {
	background-color: var(--carme-btn-bg);
  border-color: var(--carme-btn-border);
  box-shadow: var(--carme-btn-shadow);
}
.btn-custom:active, 
.btn-outline-secondary.custom-btn.active {
	box-shadow: none!important;
}
.btn-custom:focus, 
.btn-custom.focus {
  background-color:var(--carme-btn-bg);
  border-color: var(--carme-btn-border);
  box-shadow: var(--carme-btn-shadow);
}
.btn-custom:not(:disabled):not(.disabled):active:focus{
  background-color:var(--carme-btn-bg-hover);
  color:var(--carme-btn-text-hover);
  border-color:var(--carme-btn-bg);
}
.btn-custom:not(:disabled):not(.disabled).active.focus{
  background-color:var(--carme-btn-bg-hover);
  color:var(--carme-btn-text-hover);
  border-color:var(--carme-btn-bg);}

/* modal-[x]-button */
.btn-close {
  background: var(--carme-modal-btn-close-bg);
  border: 1px solid var(--carme-modal-btn-close-border);
  box-shadow: var(--carme-modal-btn-close-shadow)!important; 
  -webkit-box-shadow: var(--carme-modal-btn-close-shadow)!important;
  opacity: 1;
}
.btn-close:active,
.btn-close:focus {
  box-shadow: var(--carme-modal-btn-close-shadow)!important; 
  -webkit-box-shadow: var(--carme-modal-btn-close-shadow)!important;
  opacity: 0.5; 
}




.btn-outline-secondary.custom-btn {
	background-color: var(--carme-btn-bg);
  border-color: var(--carme-btn-border);
  box-shadow: var(--carme-btn-shadow);
}
.btn-outline-secondary.custom-btn:active, 
.btn-outline-secondary.custom-btn.active {
	box-shadow: none !important;
}
.btn-outline-secondary.custom-btn:focus, 
.btn-outline-secondary.custom-btn.focus {
  background-color:var(--carme-btn-bg);
  border-color: var(--carme-btn-border);
  box-shadow: var(--carme-btn-shadow);
}
.btn-outline-secondary.custom-btn:not(:disabled):not(.disabled):active:focus{
  background-color:var(--carme-btn-bg-hover);
  color:var(--carme-btn-text-hover);
  border-color:var(--carme-btn-bg);
}
.btn-outline-secondary.custom-btn:not(:disabled):not(.disabled).active.focus{
  background-color:var(--carme-btn-bg-hover);
  color:var(--carme-btn-text-hover);
  border-color:var(--carme-btn-bg);}

/*--- progress bar ---*/
.progress {
  background-color: var(--carme-progress-bar-bg);
  border: 1px solid var(--carme-progress-bar-border);
  box-shadow: var(--carme-progress-bar-shadow);  
}
.progress-bar.bg-used{
  background: var(--carme-blue); 
}
.progress-bar.bg-queued{
  background: var(--carme-black);
}
.progress-bar.bg-cpu{
  background: var(--carme-orange);
}


/*--------------------------------------------------*/
/*                 Overlayscrollbar                 */
/*--------------------------------------------------*/

/* uses overlay scrollbar */
.scrollbar-overlay {
  overflow: hidden;
}
.os-theme-dark > .os-scrollbar > .os-scrollbar-track > .os-scrollbar-handle {
  border-radius:0;
  background-color: var(--carme-scrollbar-bg);
}
.os-theme-dark > .os-scrollbar:hover > .os-scrollbar-track > .os-scrollbar-handle, .os-theme-dark > .os-scrollbar:focus > .os-scrollbar-track > .os-scrollbar-handle {
  background: var(--carme-scrollbar-bg);
}
/* hides native scrollbar */
.os-theme-dark {
  -ms-overflow-style: none; 
  scrollbar-width: none;     
}
.os-theme-dark::-webkit-scrollbar { 
  display: none; 
}

