}

/* i-circles */
.i-blue {
  background: rgba(var(--carme-blue-rgb),0.08);
  color: var(--carme-blue);
}
.i-gray {
  background: rgba(var(--carme-gray-rgb),0.08);
  color: var(--carme-gray);
} 
.i-green {
  background: rgba(var(--carme-green-rgb),0.08);
  color: var(--carme-green);
}
.i-cyan {
  background: rgba(var(--carme-cyan-rgb),0.08);
  color: var(--carme-cyan);
}
.i-orange {
  background: rgba(var(--carme-orange-rgb),0.08);
