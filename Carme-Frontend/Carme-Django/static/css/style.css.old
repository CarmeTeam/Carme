}
.fs-5{
  font-size: 1.425rem!important;
}
.fs-6{
  font-size: 1.525rem!important;
}
.fs-7{
  font-size: 1.625rem!important;
}
.fs-8{
  font-size: 1.725rem!important;
}
.fs-9{
  font-size: 1.825rem!important;
}
.fs-10{
  font-size: 1.925rem!important;
}
.fs-20{
