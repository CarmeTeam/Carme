}
.text-secondary{
  color:var(--carme-secondary)!important;
}
.text-success{
  color:var(--carme-success)!important;
}
.text-disabled{
  color:var(--carme-disabled)!important;
}
.text-warning{
  color:var(--carme-warning)!important;
}
.text-danger{
  color:var(--carme-danger)!important;
}
.text-info{
  color:var(--carme-info)!important;
}
.text-light{
  color:var(--carme-light)!important;
}
.text-dark{
  color:var(--carme-dark)!important;
}

/* text-rounded-pill */
.rounded-pill-1{
  border-radius: 0.1rem!important;
}
.rounded-pill-2{
